<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="495dp"
    tools:context=".presentation.onboarding.term.TermFragment">

    <TextView
        android:id="@+id/tv_termTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/TermTitle"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_marginTop="40dp"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/tv_termSubTitle"
        android:layout_width="245dp"
        android:layout_height="45dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="5dp"
        android:text="@string/TermMiddleTitle"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_termTitle" />

    <TextView
        android:id="@+id/tv_allAccept"
        android:layout_width=""
        android:layout_height=""


</androidx.constraintlayout.widget.ConstraintLayout>